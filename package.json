{
  "name": "vite-plugin-add-scripts",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Reinier Kaper",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc -p . -w --incremental",
    "build": "rimraf dist && run-s build-bundle build-types",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --target=node12 --outfile=dist/index.js",
    "build-types": "tsc -p . --emitDeclarationOnly --outDir temp && api-extractor run && rimraf temp",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package plugin-vue",
    "release": "node ../../scripts/release.js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheDutchCoder/vite-plugin-add-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/TheDutchCoder/vite-plugin-add-scripts/issues"
  },
  "homepage": "https://github.com/TheDutchCoder/vite-plugin-add-scripts#readme",
  "devDependencies": {
    "rollup": "^2.38.5",
    "vite": "^2.2.4"
  }
}
